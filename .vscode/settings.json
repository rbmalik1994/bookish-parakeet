{
    //git
    "git.autofetch": true,
    "git.suggestSmartCommit": true,
    // workbench
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorTheme": "Dracula Theme",
    "workbench.colorCustomizations": {
        "editorGroup.border": "#12f78a",
        "panel.border": "#12f78a",
        "sash.hoverBorder": "#12f78a",
        "sideBar.border": "#12f78a",
        "statusBar.background": "#07cf70",
        "statusBar.border": "#07cf70",
        "statusBar.debuggingBackground": "#cf0766",
        "statusBar.debuggingBorder": "#cf0766",
        "statusBarItem.hoverBackground": "#059e55",
        "statusBarItem.remoteBackground": "#07cf70"
    },
    "workbench.tips.enabled": false,
    "workbench.sideBar.location": "right",
    "workbench.startupEditor": "none",
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.enablePreviewFromCodeNavigation": true,
    "workbench.editor.enablePreviewFromQuickOpen": true,
    "workbench.editor.openPositioning": "last",
    "window.confirmBeforeClose": "keyboardOnly",
    "workbench.editor.revealIfOpen": true,
    //explorer
    "explorer.autoReveal": true,
    "explorer.autoRevealExclude": {
        "**/node_modules": true,
        "**/bower_components": true
    },
    //editor 
    "editor.minimap.autohide": true,
    "editor.minimap.enabled": false,
    "editor.minimap.side": "left",
    "editor.minimap.renderCharacters": false,
    "editor.formatOnPaste": false,
    "editor.formatOnType": false,
    "editor.stickyScroll.enabled": true,
    "editor.inlineSuggest.showToolbar": "always",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.wordWrap": "off",
    "editor.quickSuggestions": {
        "other": "on",
        "comments": "on",
        "strings": "on"
    },
    // files 
    "files.autoSave": "afterDelay",
    // Terminal 
    "terminal.integrated.shellIntegration.enabled": false,
    // AutoDoctString 
    "autoDocString.includeName": true,
    //json 
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    //Python 
    "[python]": {
        "diffEditor.ignoreTrimWhitespace": false,
        "editor.formatOnType": true,
        "editor.wordBasedSuggestions": "off"
    },
    "python.analysis.autoFormatStrings": true,
    // Peacock
    "peacock.affectEditorGroupBorder": true,
    "peacock.keepBadgeColor": true,
    "peacock.keepForegroundColor": true,
    "peacock.surpriseMeOnStartup": true,
    "peacock.affectDebuggingStatusBar": true,
    "peacock.affectPanelBorder": true,
    "peacock.affectStatusAndTitleBorders": true,
    "peacock.affectTitleBar": false,
    "peacock.affectSideBarBorder": true,
    "peacock.affectActivityBar": false,
    //TODO Highlight
    "todohighlight.toggleURI": true,
    "todohighlight.isCaseSensitive": false,
    "todohighlight.include": [
        "**/*.js",
        "**/*.jsx",
        "**/*.ts",
        "**/*.tsx",
        "**/*.html",
        "**/*.php",
        "**/*.css",
        "**/*.scss",
        "**/*.txt",
        "**/*.mdown",
        "**/*.md",
        "**/*.py"
    ],
    // TODO Tre 
    "todo-tree.general.showActivityBarBadge": true,
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIX",
        "FIXME",
        "FIXIT",
        "MARK",
        "todo",
        "ToDo",
        "tOdO",
        "TODO",
        "XXX",
        "[ ]",
        "[x]",
        "QUES",
        "ANS",
        "NOTE",
        "REVIEW",
        "REF",
        "WARN"
    ],
    "todo-tree.highlights.backgroundColourScheme": [
        "red",
        "orange",
        "yellow",
        "green",
        "blue",
        "indigo",
        "violet",
        "black",
        "white"
    ],
    "todo-tree.general.tagGroups": {
        "FIX": [
            "FIXME",
            "FIXIT"
        ],
        "TODO": [
            "TODO",
            "todo",
            "ToDo",
            "tOdO"
        ]
    },
    "todo-tree.highlights.defaultHighlight": {
        "type": "tag-and-comment",
        "fontWeight": "bold",
        "foreground": "#000000",
        "opacity": 90,
    },
    "todo-tree.highlights.customHighlight": {
        "NOTE": {
            "background": "#FFA500",
            "iconColour": "#FFA500",
            "gutterIcon": true,
            "icon": "file",
            "type": "text"
        },
        "TODO": {
            "background": "#FFD703",
            "iconColour": "#FFD703",
            "icon": "tasklist",
            "gutterIcon": true
        },
        "BUG": {
            "background": "#ff000080",
            "iconColour": "#ff000080",
            "gutterIcon": true,
            "icon": "bug"
        },
        "HACK": {
            "background": "#ff0080",
            "iconColour": "#ff0080",
            "gutterIcon": true,
            "icon": "tools"
        },
        "FIX": {
            "background": "#F80B2180",
            "iconColour": "#F80B2180",
            "gutterIcon": true,
            "icon": "flame"
        },
        "MARK": {
            "background": "#157EFB",
            "iconColour": "#157EFB",
            "gutterIcon": true,
            "icon": "tag",
        },
        "XXX": {
            "background": "#FFA500",
            "iconColour": "#FFA500",
            "gutterIcon": true,
            "icon": "x"
        },
        "[ ]": {
            "background": "#008000",
            "iconColour": "#008000",
            "gutterIcon": true,
            "icon": "issue-draft"
        },
        "[x]": {
            "background": "#0000ff",
            "iconColour": "#0000ff",
            "gutterIcon": true,
            "icon": "issue-closed"
        },
        "QUES": {
            "background": "#000",
            "foreground": "#fff",
            "iconColour": "#000",
            "gutterIcon": true,
            "icon": "question"
        },
        "ANS": {
            "background": "#FFF",
            "iconColour": "#FFF",
            "gutterIcon": true,
            "icon": "pencil"
        },
        "REVIEW": {
            "background": "#00ffff",
            "foreground": "#000",
            "iconColour": "#add8e6",
            "gutterIcon": true,
            "icon": "eye"
        },
        "REF": {
            "foreground": "#000",
            "background": "#808080",
            "iconColour": "#808080",
            "gutterIcon": true,
            "icon": "link",
            "type": "tag"
        },
        "WARN": {
            "foreground": "#000",
            "background": "#FFD703",
            "gutterIcon": true,
            "icon": "alert",
            "iconColour": "#FFD703"
        }
    },
    "terminal.integrated.defaultProfile.linux": "zsh",
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "projectManager.git.baseFolders": [
        "/workspaces/Magma-Chaos./github/Project"
    ],
    "githubPullRequests.remotes": [
        "origin",
        "upstream",
        "main",
        "remotes",
        "remotes/origin"
    ],
    "githubIssues.createIssueTriggers": [
        "TODO",
        "todo",
        "BUG",
        "FIXME",
        "ISSUE",
        "HACK",
        "*"
    ],
    "githubPullRequests.queries": [
        {
            "label": "Waiting For My Review",
            "query": "is:open review-requested:${user} repo:${repository}"
        },
        {
            "label": "Assigned To Me",
            "query": "is:issue is:open repo:${repository}"
        },
        {
            "label": "Created By Me",
            "query": "is:open author:${user} repo:{$org}/${repository}"
        },
        {
            "label": "Mentioned By Me",
            "query": "is:open mentions:${user} repo:{$org}/${repository}"
        }
    ],
    "githubIssues.queries": [
        {
            "label": "My Issues",
            "query": "is:open  assigne:${user}"
            // ,
            // "groupBy": [
            //     "milestone"
            // ]
        },
        {
            "label": "All",
            "query": "is:open"
        },
        {
            "label": "Created Issues",
            "query": "author:${user} state:open repo:${org}/${repository} sort:created-desc"
        },
        {
            "label": "Recent Issues",
            "query": "state:open repo:${org}/${repository} sort:updated-desc user:${user}"
        }
    ],
    "projectManager.projectsLocation": "/workspaces/Magma-Chaos/.github/",
    "projectManager.vscode.baseFolders": [
        "/workspaces/Magma-Chaos./github/Project"
    ],
    "githubIssues.issueBranchTitle": "${org}/${repository}/issue/${issueNumber}",
    "githubPullRequests.showPullRequestNumberInTree": true,
    "github.experimental.multipleAccounts": true,
    "python.analysis.inlayHints.functionReturnTypes": true,
    "python.analysis.inlayHints.pytestParameters": true,
    "python.analysis.inlayHints.variableTypes": true,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[css]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },
    "vsicons.dontShowNewVersionMessage": true,
    "files.associations": {
        ".env*": "dotenv",
        "*.css": "tailwindcss"
    },
    "editor.tokenColorCustomizations": {
        "[*Light*]": {
            "textMateRules": [
                {
                    "scope": "ref.matchtext",
                    "settings": {
                        "foreground": "#000"
                    }
                }
            ]
        },
        "[*Dark*]": {
            "textMateRules": [
                {
                    "scope": "ref.matchtext",
                    "settings": {
                        "foreground": "#fff"
                    }
                }
            ]
        },
        "textMateRules": [
            {
                "scope": "keyword.other.dotenv",
                "settings": {
                    "foreground": "#FF000000"
                }
            }
        ]
    },
    "python.terminal.activateEnvInCurrentTerminal": true,
    "path-intellisense.showHiddenFiles": true,
    "files.exclude": {
        "**/.git": false
    },
    "python.createEnvironment.contentButton": "show",
    "python.terminal.executeInFileDir": true,
    "python.terminal.launchArgs": [],
    "github.copilot.chat.agent.enabled": true,
    "sqldeveloper.sqlHistory.historyLimit": 500,
    "editor.formatOnSave": false,
    "github.copilot.chat.edits.codesearch.enabled": true,
    "github.copilot.nextEditSuggestions.enabled": true,
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "sqldeveloper.telemetry.enabled": false,
    "chat.renderRelatedFiles": true,
    "inlineChat.lineEmptyHint": true,
    "sqldeveloper.database.nls.dateFormat": "DD/MM/RRRR",
    "sqldeveloper.database.nls.timestampFormat": "DD/MM/RRRR HH24:MI:SSXFF",
    "sqldeveloper.database.nls.timestampTZ Format": "DD/MM/RRRR HH24:MI:SSXFF TZR",
    "sqldeveloper.format.alignment": {
        "assignmentOperator": true
    },
    "cSpell.ignorePaths": [
        "package-lock.json",
        "node_modules",
        "vscode-extension",
        ".git/{info,lfs,logs,refs,objects}/**",
        ".git/{index,*refs,*HEAD}",
        ".vscode",
        ".vscode-insiders",
        ".gitignore"
    ],
    "peacock.remoteColor": "#07cf70",
    "nxConsole.generateAiAgentRules": true,
}